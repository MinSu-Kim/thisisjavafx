<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="180.0" prefWidth="520.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kr.or.dgit.thisisjavafx.view_controll.ViewController">
   <children>
      <HBox alignment="TOP_CENTER" spacing="10.0">
         <children>
            <VBox alignment="CENTER" prefHeight="176.0" prefWidth="100.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" text="ListView" />
                  <ListView fx:id="listView" prefHeight="151.0" prefWidth="100.0" />
               </children>
            </VBox>
            <VBox alignment="CENTER" prefHeight="100.0" prefWidth="290.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" text="TableView" VBox.vgrow="ALWAYS" />
                  <TableView fx:id="tableView" prefHeight="100.0" prefWidth="290.0" VBox.vgrow="ALWAYS">
                     <columns>
                        <TableColumn prefWidth="124.0000137090683" text="스마트폰" />
                        <TableColumn prefWidth="118.39997100830078" text="이미지" />
                     </columns>
                  </TableView>
               </children>
            </VBox>
            <VBox alignment="TOP_CENTER" prefHeight="176.0" prefWidth="87.0" spacing="10.0">
               <children>
                  <Label alignment="CENTER" text="ImageView" />
                  <ImageView fx:id="imageView" fitHeight="150.0" fitWidth="60.0" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
         </children>
      </HBox>
      <HBox alignment="CENTER" spacing="20.0">
         <children>
            <Button mnemonicParsing="false" onAction="#handleBtnOkAction" text="확인" />
            <Button mnemonicParsing="false" onAction="#handleBtnCancelAction" text="취소" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
